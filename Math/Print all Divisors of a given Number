#include <bits/stdc++.h>
using namespace std;

/* =====================================================
   ðŸ”´ Approach 1: Brute Force
   Time: O(N)
===================================================== */
vector<int> divisorsBrute(int n) {
    vector<int> res;

    for (int i = 1; i <= n; i++) {
        if (n % i == 0)
            res.push_back(i);
    }
    return res;
}

/* =====================================================
   ðŸŸ¡ Approach 2: âˆšN Optimized (unsorted)
===================================================== */
vector<int> divisorsSqrt(int n) {
    vector<int> res;

    for (int i = 1; i * i <= n; i++) {
        if (n % i == 0) {
            res.push_back(i);

            if (i != n / i)
                res.push_back(n / i);
        }
    }
    return res;
}

/* =====================================================
   ðŸŸ¢ Approach 3: âˆšN Sorted (BEST)
===================================================== */
vector<int> divisorsSorted(int n) {
    vector<int> small, large;

    for (int i = 1; i * i <= n; i++) {
        if (n % i == 0) {
            small.push_back(i);

            if (i != n / i)
                large.push_back(n / i);
        }
    }

    reverse(large.begin(), large.end());
    small.insert(small.end(), large.begin(), large.end());

    return small;
}

/* =====================================================
   ðŸ”µ Approach 4: Direct Print (Topper Style)
===================================================== */
void printDivisorsDirect(int n) {
    vector<int> large;

    for (int i = 1; i * i <= n; i++) {
        if (n % i == 0) {
            cout << i << " ";

            if (i != n / i)
                large.push_back(n / i);
        }
    }

    for (int i = large.size() - 1; i >= 0; i--)
        cout << large[i] << " ";

    cout << "\n";
}

/* ---------- Utility ---------- */
void printVector(const vector<int>& v) {
    for (int x : v) cout << x << " ";
    cout << "\n";
}

/* =================== MAIN =================== */
int main() {
    int n;
    cout << "Enter number: ";
    cin >> n;

    cout << "\nðŸ”´ Brute Force:\n";
    printVector(divisorsBrute(n));

    cout << "\nðŸŸ¡ âˆšN Optimized (unsorted):\n";
    printVector(divisorsSqrt(n));

    cout << "\nðŸŸ¢ âˆšN Sorted (Best):\n";
    printVector(divisorsSorted(n));

    cout << "\nðŸ”µ Direct Print (Topper Style):\n";
    printDivisorsDirect(n);

    return 0;
}
